<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>UserController</servlet-name>
  	<display-name>UserController</display-name>
  	<description></description>
  	<servlet-class>com.mega_city_cab.controller.UserController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CustomerController</servlet-name>
  	<display-name>CustomerController</display-name>
  	<description></description>
  	<servlet-class>com.mega_city_cab.controller.CustomerController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookingController</servlet-name>
  	<display-name>BookingController</display-name>
  	<description></description>
  	<servlet-class>com.mega_city_cab.controller.BookingController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>DriverController</servlet-name>
  	<display-name>DriverController</display-name>
  	<description></description>
  	<servlet-class>com.mega_city_cab.controller.DriverController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>UserController</servlet-name>
  	<url-pattern>/User</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CustomerController</servlet-name>
  	<url-pattern>/Customer</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookingController</servlet-name>
  	<url-pattern>/booking</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>DriverController</servlet-name>
  	<url-pattern>/Driver</url-pattern>
  </servlet-mapping>
</web-app>
